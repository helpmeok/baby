<view class="container"><cu-custom vue-id="1" bgColor="bg-gradual-red" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText"></view><view slot="content">{{cuText}}</view></cu-custom><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><mpvue-city-picker class="vue-ref" vue-id="2" themeColor="{{themeColor}}" pickerValueDefault="{{cityPickerValueDefault}}" addressList="{{addressList}}" data-ref="mpvueCityPicker" data-event-opts="{{[['^onCancel',[['onCancel']]],['^onConfirm',[['onConfirm']]]]}}" bind:onCancel="__e" bind:onConfirm="__e" bind:__l="__l"></mpvue-city-picker><view class="bg-white list-box"><view class="list-cell flex-r-between pd-box"><view class="list-cell-left ">收货人</view><view class="list-cell-right"><input class="align-right" type="text" name="consignee" placeholder-class="gray" placeholder="请填写姓名" data-event-opts="{{[['input',[['__set_model',['$0','consignee','$event',[]],['model']]]]]}}" value="{{model.consignee}}" bindinput="__e"/></view></view><view class="list-cell flex-r-between pd-box"><view class="list-cell-left ">联系电话</view><view class="list-cell-right"><input class="align-right" type="number" name="mobile" placeholder-class="gray" placeholder="请填写电话" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['model']]]]]}}" value="{{model.mobile}}" bindinput="__e"/></view></view><view class="list-cell flex-r-between pd-box"><view class="list-cell-left ">所在地区</view><view data-event-opts="{{[['tap',[['showMulLinkageThreePicker',['$event']]]]]}}" class="list-cell-right" bindtap="__e">{{''+address+''}}<input hidden="{{!(false)}}" type="text" name="province" data-event-opts="{{[['input',[['__set_model',['$0','province','$event',[]],['model']]]]]}}" value="{{model.province}}" bindinput="__e"/></view></view><view class="list-cell pd-box"><view class="list-cell-left ">详细地址</view><view class="list-cell-right" style="margin-top:20rpx;"><view class="uni-textarea"><textarea placeholder="请输入街道门牌号" placeholder-class="gray" name="address" data-event-opts="{{[['input',[['__set_model',['$0','address','$event',[]],['model']]]]]}}" value="{{model.address}}" bindinput="__e"></textarea></view></view></view></view><view class="flex-r-between bg-white pd-box"><text class="blod">设置为默认地址</text><switch style="margin:20rpx;" checked="{{model.is_default==1}}" color="#FC4041" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><view class="fixed-bottom fixed-bottom-height flex-r-center bg-default-color"><button class="btn-submit flex-r-center bg-default-color white" plain="none" formType="submit" type="primary" hover-class="none">创建</button></view></form></view>