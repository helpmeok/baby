<view><cu-custom vue-id="1" bgColor="bg-gradual-red" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText"></view><view slot="content">屏蔽的作者</view></cu-custom><block wx:if="{{list.length==0}}"><empty vue-id="2" msg="您还没有屏蔽的作者~" bind:__l="__l"></empty></block><view class="container"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list-item flex-r-between"><view class="flex"><image style="width:100rpx;height:100rpx;border-radius:50%;margin-right:20rpx;" src="{{item.avatar}}" mode="widthFix"></image><view><view class="font-b">{{item.name}}</view><view class="gray "><text style="margin-right:20rpx;">{{item.originalNum+"篇原创"}}</text><text>{{item.fansNum+"粉丝"}}</text></view></view></view><uni-tag vue-id="{{'3-'+index}}" text="取消屏蔽" type="error" circle="{{true}}" data-event-opts="{{[['^click',[['cancel',['$0',index],[[['list','',index,'userId']]]]]]]}}" bind:click="__e" bind:__l="__l"></uni-tag></view></block><block wx:if="{{list.length>0}}"><view class="uni-tab-bar-loading"><uni-load-more vue-id="4" loadingType="{{loadingType}}" contentText="{{loadingText}}" bind:__l="__l"></uni-load-more></view></block></view></view>